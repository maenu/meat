"
	Interpreter
	
	Interpreter that looks up variables in the context.
"
interpreter := ({
	(aSelector = ':=') ifTrue: {
		aReceiver value: (someArguments at: 1)
	} ifFalse: {
		aReceiver receive: aSelector with: someArguments in: aSender
	}
} with: ['aSelector' 'someArguments' 'aReceiver' 'aSender'])